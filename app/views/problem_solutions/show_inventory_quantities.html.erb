<%= @title = "Inventory Quantities Results" %>
<table>
  <tr>
    <th>Product name</th>
    <th>Period name</th>
    <th>Inventory quantity</th>
  </tr>

  <% InventorySolution.all.each do |di| %>

  <tr>
  <td><%= di.product_name %></td>
  <td><%= di.period_name %></td>
  <td><%= di.inventory %></td>
  </tr>
   <% end %>
</table>
